c:Button("EXTRA TOOLS (inf)",function()
    local tempr = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
    wait(.01)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-178.651, 431.8, 5510.24)
    wait(.1)
    fireclickdetector(snkm.Selection1.ClickDetector)
    fireclickdetector(snkm.Selection2.ClickDetector)
    fireclickdetector(snkm.Selection3.ClickDetector)
    fireclickdetector(snkm.Selection4.ClickDetector)
    fireclickdetector(snkm.Selection5.ClickDetector)
    fireclickdetector(snkm.Selection6.ClickDetector)
    fireclickdetector(snkm.Selection7.ClickDetector)
    fireclickdetector(snkm.Selection8.ClickDetector)
    wait(1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(tempr)
end)
