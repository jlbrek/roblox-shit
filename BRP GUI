c:Button("REMOVE DRUMSET",function()
    if ws["Custom heavy drum"] ~= nil then
        ws["Custom heavy drum"]:Destroy()
    elseif ws["Custom heavy drum"] == nil then
        sgui:SetCore("SendNotification",{Title = ":skull:", Text = "Dawg, Xplaymxne666, you already pressed this shit lmao", Durriation = 2})
    end
end)

c:Slider("# of times",{
    min = 1; -- min value of the slider
    max = 20; -- max value of the slider
    precise = false; -- max 2 decimals
},function(vvalue)
    numbroftimes = vvalue
end)

c:Button("EXTRA TOOLS (1-20)",function() -- Eventually want to make this a toggle instead of a button
    for numtps = 0, numbroftimes, 1 do
        local TempLocation = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        wait(0.01)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-178.651, 431.8, 5510.24) -- optimal location to grab all of the tools
        wait(0.5)
        fireclickdetector(snkm.Selection1.ClickDetector)
        wait(0.001)
        fireclickdetector(snkm.Selection2.ClickDetector)
        wait(0.001)
        fireclickdetector(snkm.Selection3.ClickDetector)
        wait(0.001) 
        fireclickdetector(snkm.Selection4.ClickDetector)
        wait(0.001)
        fireclickdetector(snkm.Selection5.ClickDetector)
        wait(0.001) 
