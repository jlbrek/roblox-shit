    local TempLocation = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
    wait(0.01)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-178.651, 431.8, 5510.24) -- optimal location to grab all of the tools
    wait(0.1)
    fireclickdetector(snkm.Selection1.ClickDetector)
    wait(0.001)
    fireclickdetector(snkm.Selection2.ClickDetector)
    wait(0.001)
    fireclickdetector(snkm.Selection3.ClickDetector)
    wait(0.001) --I FUCKING LOVE MEN
    fireclickdetector(snkm.Selection4.ClickDetector)
    wait(0.001)
